{
  "name": "ashrohcorporatewebsite",
  "version": "1.0.0",
  "description": "A corporate website for Ashroh Enterprises Inc.",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "dev": "concurrently --kill-others \"webpack-dev-server --inline --hot\" \"nodemon ./server.js\"",
    "createDatabase": "node ./server/model/initial/createDatabase.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --mode=production",
    "prod": "node ./server.js",
    "server": "pm2 start ./server.js --name \"AshrohCorporateSite\" ",
    "checklint": "eslint --ignore-path .gitignore .",
    "fixlint": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "makepretty": "./node_modules/.bin/prettier --write --ignore-path .gitignore '**/*.js' '**/*.vue'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomrembrown/ashrohCorporateWebsite.git"
  },
  "keywords": [
    "community",
    "loneliness",
    "diversity",
    "corporate",
    "triple",
    "bottom",
    "line",
    "social",
    "economy"
  ],
  "author": "Tom Brown <tom@tomrembrown.com> (www.tomrembrown.com)",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/tomrembrown/ashrohCorporateWebsite/issues"
  },
  "homepage": "https://github.com/tomrembrown/ashrohCorporateWebsite#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.25",
    "concurrently": "^4.1.1",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.1.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "md5": "^2.2.1",
    "microdata-node": "^1.0.0",
    "nodemailer": "^6.3.0",
    "nodemailer-mailgun-transport": "^1.4.0",
    "pg": "^7.12.0",
    "pm2": "^3.5.1",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1",
